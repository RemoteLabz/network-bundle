{
    "name": "remotelabz/network-bundle",
    "description": "NetworkBundle is a bundle used by RemoteLabz to handle all IP and network related aspects.",
    "type": "symfony-bundle",
    "require": {
        "php": "^7.3|^8.0",
        "jms/serializer": "^1.13|^2.0|^3.0",
        "doctrine/orm": "^2.7",
        "doctrine/annotations": "^1.10"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.1",
        "squizlabs/php_codesniffer": "3.*"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "remotelabz\\NetworkBundle\\": ""
        },
        "exclude-from-classmap": [
            "Resources/",
            "Tests/"
        ]
    },
    "authors": [
        {
            "name": "Julien Hubert",
            "email": "julien.hubert1@univ-reims.fr"
        },
        {
            "name": "Florent Nolot",
            "email": "florent.nolot@univ-reims.fr"
        }
    ]
}
